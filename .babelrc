{
  "presets": [
    "react",
    "stage-2",
    ["env", {
      "targets": {
        "browsers": ["last 2 versions", "safari 7", "IE 11"]
      },
      "useBuiltIns": false,
      "modules": false,
      "blacklist": [
        "transform-regenerator"
      ]
    }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        "react",
        "stage-2",
        ["env", {
          "targets": {
            "browsers": ["last 2 versions", "safari 7", "IE 11"]
          },
          "useBuiltIns": false,
          "modules": "commonjs",
          "blacklist": [
            "transform-regenerator"
          ]
        }
        ]
      ]
    }
  },
  "plugins": ["emotion"]
}

